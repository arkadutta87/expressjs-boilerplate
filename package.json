{
  "name": "expressjs-boilerplate",
  "version": "1.3.10",
  "description": "Boilerplate for expressjs based application",
  "repository": "git@github.com:360fy/expressjs-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && mkdir -p lib/server && cp -R src/server/views/ lib/server/views && babel -d lib/ src/",
    "postinstall": "npm run compile",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "cli-table2": "0.2.0",
    "command-line-boilerplate": "1.1.8",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "debug": "2.3.2",
    "emailjs": "^1.0.8",
    "events": "1.1.1",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "express-session": "^1.14.2",
    "express-winston": "2.0.0",
    "handlebars": "4.0.6",
    "immutable": "3.8.1",
    "lodash": "4.17.2",
    "mongodb": "^2.2.16",
    "monk": "^3.1.3",
    "ms": "0.7.2",
    "passwordless": "^1.1.2",
    "passwordless-mongostore": "^0.1.4",
    "performance-now": "0.2.0",
    "qs": "6.3.0",
    "serve-favicon": "2.3.2",
    "socket.io": "1.5.1",
    "socket.io-client": "1.5.1",
    "winston": "2.3.0",
    "winston-daily-rotate-file": "1.3.1"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.16.0",
    "eslint": "3.10.2",
    "eslint-config-360fy": "1.1.12",
    "eslint-plugin-react": "6.7.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
